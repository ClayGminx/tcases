{
    "openapi": "3.0.0",
    "info": {
        "title": "OpenAPI Request Test Cases",
        "version": "0.0.0"
    },
    "paths": {
        "/posts": {
            "delete": {
                "parameters": [
                    {
                        "name": "Post-Id",
                        "required": true,
                        "in": "cookie",
                        "style": "spaceDelimited",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "enum": [ "Why?", "@Home", "/Because/"]
                            },
                            "minItems": 1,
                            "maxItems": 2,
                            "uniqueItems": true
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/success"
                    },
                    "default": {
                        "$ref": "#/components/responses/failure"
                    }
                }
            }
        }
    },
    "components": {
        "requestBodies": {
            "standard": {
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object"
                        }
                    }
                }
            }
        },
        "responses": {
            "success": {
                "description": "Success",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object"
                        }
                    }
                }
            },
            "failure": {
                "description": "Error",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object"
                        }
                    }
                }
            }
        }
    }
}
